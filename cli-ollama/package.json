    "type-check": "tsc -p tsconfig.json --noEmit"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4"
  }
}
